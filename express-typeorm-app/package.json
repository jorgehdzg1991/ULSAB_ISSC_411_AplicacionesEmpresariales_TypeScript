    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.11.11",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
