    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^27.4.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.13",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.3.0",
    "tsc": "^2.0.3",
    "typescript": "^4.5.2"
  }
